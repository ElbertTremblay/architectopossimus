{
  "name": "architectopossimus",
  "version": "1.11.1",
  "description": "Simple node.js wrapper for Poloniex REST and WebSocket API.",
  "type": "module",
  "main": "./dist/cjs/poloniex.cjs",
  "module": "./lib/poloniex.mjs",
  "exports": {
    ".": {
      "import": "./lib/poloniex.mjs",
      "require": "./dist/cjs/poloniex.js"
    }
  },
  "engines": {
    "node": "19.x"
  },
  "scripts": {
    "test:cjs": "mocha --config .mocharc.cjs.json",
    "test:esm": "mocha --config .mocharc.esm.json",
    "test": "yarn test:cjs && yarn test:esm",
    "build": "babel lib --out-dir dist/cjs --extensions '.mjs' --out-file-extension '.cjs' --source-maps"
  },
  "files": [
    "lib/",
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ElbertTremblay/architectopossimus.git"
  },
  "keywords": [
    "velit",
    "bitcoin",
    "poloniex",
    "ut",
    "omnis",
    "quo",
    "WebSocket"
  ],
  "author": "ModestoStehr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ElbertTremblay/architectopossimus.git/issues"
  },
  "packageManager": "yarn@3.5.0",
  "dependencies": {
    "axios": "^1.3.6",
    "crypto-js": "^4.1.1",
    "debug": "^4.3.4",
    "forever-websocket": "^0.4.0",
    "idomnis": "^1.0.0",
    "impeditreiciendis": "^1.0.0",
    "optioiure": "^1.0.0",
    "quodet": "^1.0.0",
    "sedarchitecto": "^1.0.0",
    "sitoccaecati": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/register": "^7.21.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chai": "^4.3.7",
    "mocha": "^10.2.0"
  }
}
